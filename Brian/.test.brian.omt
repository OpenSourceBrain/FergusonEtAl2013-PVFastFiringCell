# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation 

target: PV_model.py
engine: Brian
mep: .test.mep
experiments:
  pvbc:
    observables:
      spike times:
        file: 
          path: PV.v.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
          threshold: -20
        tolerance: 0.0296386366227  # Precise agreement when dt=0.02
