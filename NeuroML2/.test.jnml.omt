# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation 

target: LEMS_PVBC.xml
engine: jNeuroML
mep: ../Brian/.test.mep
experiments:
  pvbc:
    observables:
      spike times:
        file: 
          path: LEMS_PVBC.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
          threshold: 0
        tolerance: 0.00210574386036
